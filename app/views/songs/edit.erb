<form method="POST" action="/songs/<%= @song.id%>">
<input type="hidden" name="_method" value="PATCH" />
<div>
    <label>Name</label>
    <input type="text" name="song[name]" id="song[name]"></input>
</div>
<h2>Select Artist by ID</h2>
    <input type="text" name="song[artist_id]" id="song[artist_id]"></input>
<h2>Select Genre by ID</h2>
    <input type="text" name="song[genre_id]" id="song[genre_id]"></input>

<input type="submit" value="Update Song">
</form>