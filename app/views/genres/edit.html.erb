<h2>My Genre</h2>
  <form method="POST" action="/genres/<%= @genre.id %>">
    <input type="hidden" name="_method" value="PATCH"/>
    <div>
      <label>Name</label>
      <input type="text" name="genre[name]" value="<%= @genre.name %>"></input>
    </div>

    <input type="submit" value="Update Genre"/>
  </form>
