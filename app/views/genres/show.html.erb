<h1><%=@genre.name%></h1>
<h2>Songs: </h2>
    <ul>
        <%@genre.songs.uniq.each do |song|%>
            <li><%= link_to song.name, song_path(song) %></li>
        <%end%>
    </ul>
<h2>Artists: </h2>
    <ul>
        <li>
            <%@genre.artists.uniq.each do |artist|%>
            <li><%= link_to artist.name, artist_path(artist) %></li>
            <%end%>
        </li>
    </ul>

    <%= link_to 'Edit Genre', edit_genre_path(@genre) %>