<h2>Edit Artist </h2>
  <form method="POST" action="/artists/<%= @artist.id %>">
    <input type="hidden" name="_method" value="PATCH"
    <div>
      <label>Name</label>
      <input type="text" name="artist[name]" value="<%= @artist.name %>"></input>
    </div>
    <div>
      <label>Bio</label>
      <input type="text" name="artist[bio]" value="<%= @artist.bio %>"></input>
    </div>

    <input type="submit" value="Update Artist"/>
  </form>
