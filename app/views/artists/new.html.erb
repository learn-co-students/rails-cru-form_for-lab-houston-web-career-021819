<h2>Create Artist </h2>
  <form method="POST" action="/artists">
    <div>
      <label>Name</label>
      <input type="text" name="artist[name]"></input>
    </div>

    <div>
      <label>Bio</label>
      <input type="text" name="artist[bio]"></input>
    </div>

    <input type="submit" value="Create Artist"/>
  </form>
